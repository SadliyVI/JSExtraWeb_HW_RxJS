{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://servermode.vercel.app/api/$1"
        },
        {
            "source": "/bundle.js",
            "destination": "/dist/bundle.js"
        },
        {
            "source": "/dist/(.*)",
            "destination": "/dist/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/index.html"
        }
    ]
}